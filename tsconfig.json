{
  "compilerOptions": {
    "jsx": "react",
    "allowSyntheticDefaultImports": true,
    "noImplicitAny": true,
    "module": "ESNext",
    "target": "ESNext",
    "moduleResolution": "node",
    "allowJs": true,
    "noResolve": false,
    "noImplicitAny": true,
    "preserveConstEnums":true,
    "resolveJsonModule": true,
    "lib": [
        "dom",
        "es6",
        "ESNext"
    ],
  },
}

